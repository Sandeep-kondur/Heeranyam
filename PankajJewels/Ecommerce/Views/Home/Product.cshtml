@model Ecommerce.Models.ModelClasses.PostProductModel_Web
@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_MasterLayout.cshtml";

}
<!-- breadcrumb area start -->
<!-- breadcrumb area start -->
<div class="breadcrumb-area">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="breadcrumb-wrap">
                    <nav aria-label="breadcrumb">
                        <ul class="breadcrumb">
                            <li class="breadcrumb-item"><a href="~/Home/Index"><i class="fa fa-home"></i></a></li>
                            <li class="breadcrumb-item"><a href="~/Home/Listing?did=@Model.DetailCategoryId">Items Page</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Product details</li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
<input type="hidden" id="currentPId" value="@Model.ProductId" />
<!-- breadcrumb area end -->
<!-- page main wrapper start -->
<div class="shop-main-wrapper section-padding pb-0">
    <div class="container">
        <div class="row">
            <!-- product details wrapper start -->
            <div class="col-lg-12 order-1 order-lg-2">
                <!-- product details inner end -->
                <div class="product-details-inner">
                    <div class="row">
                        <div class="col-lg-5">
                            <div class="product-large-slider">
                                @if (Model.ProductDetails.Count > 0)
                                {
                                    foreach (var im in Model.ProductDetails[0].ProductDetailImages_List)
                                    {
                                        int sIndex = im.IndexOf(@".");
                                        string fType = im.Substring(sIndex);
                                        if (fType == ".gif" || fType == ".png" || fType == ".jpeg" || fType == ".jpg")
                                        {
                                            <div class="pro-large-img img-zoom">
                                                <img src="~/ProductImages/@im" alt="product-details" />
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="pro-large-img img-zoom">
                                                <video width="445" height="445" controls autoplay>
                                                    <source src="~/ProductImages/@im" type="video/mp4">
                                                    <source src="movie.ogg" type="video/ogg">
                                                    Your browser does not support the video tag.
                                                </video>
                                            </div>
                                        }

                                    }
                                }

                            </div>
                            <div class="pro-nav slick-row-10 slick-arrow-style">
                                @if (Model.ProductDetails.Count > 0)
                                {
                                    foreach (var im in Model.ProductDetails[0].ProductDetailImages_List)
                                    {
                                        int SsIndex = im.IndexOf(@".");
                                        string SfType = im.Substring(SsIndex);
                                        if (SfType == ".gif" || SfType == ".png" || SfType == ".jpeg" || SfType == ".jpg")
                                        {
                                            <div class="pro-nav-thumb">
                                                <img src="~/ProductImages/@im" alt="product-details" />
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="pro-nav-thumb">
                                                <video width="100%">
                                                    <source src="~/ProductImages/@im" type="video/mp4">
                                                    <source src="movie.ogg" type="video/ogg">
                                                    Your browser does not support the video tag.
                                                </video>

                                            </div>

                                        }

                                    }
                                }

                            </div>
                        </div>
                        <div class="col-lg-7">
                            <div class="product-details-des">
                                <div class="manufacturer-name">
                                    <a href="#">@Model.CategoryId_name > @Model.SubCategoryId_name > @Model.DetailCategoryId_name </a>
                                </div>
                                <h3 class="product-name">@Model.ProductTitle</h3>
                                <div class="ratings d-flex">
                                    <span><i class="fa fa-star-o"></i></span>
                                    <span><i class="fa fa-star-o"></i></span>
                                    <span><i class="fa fa-star-o"></i></span>
                                    <span><i class="fa fa-star-o"></i></span>
                                    <span><i class="fa fa-star-o"></i></span>
                                    <div class="pro-review">
                                        <span>1 Reviews</span>
                                    </div>
                                </div>
                                <div class="price-box">
                                    <span class="price-regular">
                                        Rs. @(Model.priceBreakup.totalAmount.ToString("#,##0.00"))

                                        <span id="talkbubble">
                                            @Model.DiscountMaster_IdName
                                        </span>
                                    </span>
                                    <div class="price-old"><del>Rs.@(Model.priceBreakup.oldAmount.ToString("#,##0.00"))</del></div>
                                </div>
                                <div class="pincodeBox">
                                    <div>
                                        Your picode
                                        <span class="example">
                                            <input type="text" placeholder="500025" value="500025" name="search">
                                            <button type="submit" class="btn btn-update">update</button>
                                        </span>
                                    </div>
                                    <p>Delivery By @Model.MaxDelivaryDays days</p>
                                </div>
                                <div class="clearfix"></div>
                                <div class="availability">
                                    <i class="fa fa-check-circle"></i>
                                    <span>@Model.ProductDescription</span>
                                </div>
                                <div class="custmize mt-1">
                                    @if (Model.IsCustomizable == true)
                                    {

                                        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                                            <div class="panel panel-default">
                                                <div class="panel-heading" role="tab" id="headingOne">
                                                    <h4 class="panel-title">
                                                        <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                                            <i class="more-less fa fa-plus"></i>
                                                            Customize this product
                                                        </a>
                                                    </h4>
                                                </div>
                                                <div id="collapseOne" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
                                                    <div class="panel-body">
                                                        <form>
                                                            <div class="d-flex">
                                                                <div>
                                                                    <div class="form-group">
                                                                        <select class="form-control" id="sel1">
                                                                            <option>White</option>
                                                                            <option>Yellow</option>
                                                                        </select>
                                                                    </div>
                                                                </div>
                                                                <div class="radiokt">
                                                                    <label class="radio-inline"><input type="radio" name="optradio" checked>14 kt</label>
                                                                    <label class="radio-inline"><input type="radio" name="optradio">18 kt</label>
                                                                    <label class="radio-inline"><input type="radio" name="optradio">22 kt</label>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    }

                                </div>

                                <div class="clearfix"></div>

                                <div class="availability">
                                    <i class="fa fa-check-circle"></i>
                                    <span>In stock</span>
                                </div>
                                <div class="quantity-cart-box d-flex align-items-center">
                                    <h6 class="option-title">qty:</h6>
                                    <div class="quantity">
                                        <div class="pro-qty"><input type="text" id="prdQty" value="1"></div>
                                    </div>
                                    <div class="action_link">
                                        <a class="btn btn-cart2" href="javascript:void(0);" onclick="AddToCart(@Model.ProductId,@Model.ProductDetails[0].ProductDetailId,-1)">Add to cart</a>
                                    </div>
                                </div>
                                @if (Model.MySizes != null)
                                {
                                    if (Model.MySizes.Count > 0)
                                    {
                                        <div class="pro-size">
                                            <h6 class="option-title">size :</h6>
                                            <select class="nice-select" onchange="GetProduct()" id="MySizeId">
                                                @foreach (var si in Model.MySizes)
                                                {
                                                    <option value="@si.ProductDetailId">@si.SizeName</option>
                                                }
                                            </select>
                                        </div>
                                    }


                                }

                                @*<div class="color-option">
                                        <h6 class="option-title">color :</h6>
                                        <ul class="color-categories">
