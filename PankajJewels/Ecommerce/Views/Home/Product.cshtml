                                        </ul>
                                    </div>*@
                                <div class="useful-links">

                                    <a href="#" data-toggle="tooltip" title="Wishlist">
                                        <i class="pe-7s-like"></i>wishlist
                                    </a>
                                </div>
                                <div class="s2">
                                    <div class="action_link">
                                        <a class="btn btn-cart" href="javascript:void(0);" onclick="ShowCustomize(@Model.ProductId,@Model.ProductDetails[0].ProductDetailId,-1)">Want to Customize? </a>
